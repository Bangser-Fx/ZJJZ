<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zjjz.ucenter.mapper.HeadPortraitMapper">

    <select id="findAllHeadPortrait" resultType="com.zjjz.model.user.Headportrait">
        SELECT * FROM headportrait
    </select>

    <select id="findHeadPortraitById" resultType="com.zjjz.model.user.Headportrait" parameterType="integer">
        SELECT * FROM headportrait WHERE hid = #{id}
    </select>
</mapper>